{
  "name": "pizza-ui",
  "version": "0.0.1",
  "description": "It's a useless UI Library",
  "author": "sword-x-shield",
  "license": "MIT",
  "keywords": [],
  "main": "dist/pizza-ui.js",
  "module": "es/index.js",
  "types": "es/src/index.d.ts",
  "scripts": {
    "dev": "pnpm run gen-declare && rollup -c --watch --bundleConfigAsCjs",
    "build": "pnpm run gen-declare && pnpm run build:component && pnpm run build:style",
    "build:component": "rimraf dist lib es && rollup -c --bundleConfigAsCjs",
    "build:test": "tsx build/build-component && pnpm run gen-types",
    "build:style": "tsx build/build-style",
    "gen-types": "npx vue-tsc --declaration --emitDeclarationOnly --project ./tsconfig.esm.json",
    "gen-declare": "tsx build/gen-declare",
    "demo": "rimraf dist lib es && tsx build/build-test.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "smooth-scroll-into-view-if-needed": "^1.1.33"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@rollup/plugin-alias": "^4.0.2",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^23.0.4",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^5.0.1",
    "@rollup/plugin-terser": "^0.2.0",
    "@types/clean-css": "^4.2.6",
    "@types/glob": "^8.0.0",
    "@types/lodash-es": "^4.17.6",
    "@types/uppercamelcase": "^3.0.0",
    "@vitejs/plugin-vue": "2.3.3",
    "clean-css": "^5.3.1",
    "esbuild": "^0.16.2",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "rimraf": "^4.1.2",
    "rollup": "^3.6.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.57.0",
    "tslib": "^2.4.1",
    "tsx": "^3.12.0",
    "unplugin-vue": "^4.0.1",
    "uppercamelcase": "^3.0.0",
    "vite": "^4.0.2",
    "vue-tsc": "^1.0.18"
  }
}
